{
    "name": "atta-1/exportable-entity-bundle",
    "type": "library",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Jlonom/EasyAdminBundle.git"
        }
    ],
    "require": {
        "php": "^8.2",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.12 || ^3.0",
        "easycorp/easyadmin-bundle": "^4.0|dev-dbal_4",
        "league/flysystem-bundle": "^3.1",
        "symfony/config": "^6.1",
        "symfony/dependency-injection": "^6.1",
        "symfony/http-kernel": "^6.1",
        "symfony/messenger": "^6.1",
        "symfony/property-access": "^6.4",
        "symfony/serializer": "^6.1"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8",
        "jetbrains/phpstorm-attributes": "^1.0",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Atta\\ExportableEntityBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atta\\ExportableEntityBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "./bin",
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": true,
            "target-directory": "vendor-bin",
            "forward-command": true
        }
    }
}
